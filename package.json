{
  "name": "passkey-cloudflare",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "deploy": "npm run build && wrangler pages deploy ./public",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.3.0",
    "@remix-run/cloudflare-pages": "^2.3.0",
    "@remix-run/css-bundle": "^2.3.0",
    "@remix-run/react": "^2.3.0",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@remix-run/dev": "^2.3.0",
    "@remix-run/eslint-config": "^2.3.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vanilla-extract/css": "^1.14.0",
    "eslint": "^8.54.0",
    "typescript": "^5.2.2",
    "wrangler": "^3.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
