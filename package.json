{
  "name": "passkey-cloudflare",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "deploy": "npm run build && wrangler pages deploy ./public",
    "lint": "run-s -c lint:*",
    "lint:typescript": "tsc",
    "lint:eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "lint:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "migrate:create": "drizzle-kit generate:sqlite --out migrations --schema app/models/schema.ts",
    "migrate:apply": "wrangler d1 migrations apply passkey-cloudflare --local",
    "migrate:apply-prod": "wrangler d1 migrations apply passkey-cloudflare"
  },
  "dependencies": {
    "@nextui-org/react": "^2.2.9",
    "@paralleldrive/cuid2": "^2.2.2",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/cloudflare-pages": "^2.3.1",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^10.16.16",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "remix-auth": "^3.6.0",
    "remix-auth-github": "git://github.com/sergiodxa/remix-auth-github.git",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "better-sqlite3": "^9.2.2",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "wrangler": "^3.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
