{
  "name": "passkey-cloudflare",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public",
    "deploy": "npm run build && wrangler pages deploy ./public",
    "typecheck": "tsc",
    "migrate:create": "drizzle-kit generate:sqlite --out migrations --schema app/models/schema.ts",
    "migrate:apply": "wrangler d1 migrations apply passkey-cloudflare --local",
    "migrate:apply-prod": "wrangler d1 migrations apply passkey-cloudflare"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@remix-run/cloudflare": "^2.3.1",
    "@remix-run/cloudflare-pages": "^2.3.1",
    "@remix-run/css-bundle": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "drizzle-zod": "^0.5.1",
    "isbot": "^3.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-github": "^1.6.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@remix-run/dev": "^2.3.1",
    "@remix-run/eslint-config": "^2.3.1",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@vanilla-extract/css": "^1.14.0",
    "better-sqlite3": "^9.1.1",
    "drizzle-kit": "^0.20.6",
    "drizzle-orm": "^0.29.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "wrangler": "^3.17.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
